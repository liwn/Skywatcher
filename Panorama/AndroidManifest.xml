<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionCode="1"
	android:versionName="1.0" package="com.skywatcher.PanoramaApp">
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:hasCode="true" android:debuggable="true">
		<activity android:label="@string/app_name" android:name="com.skywatcher.PanoramaApp.PanoramaAppActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:name="com.skywatcher.PanoramaApp.MainControlActivity"></activity>
		<activity android:name="com.skywatcher.PanoramaApp.QuickSelectActivity"></activity>
		<activity android:name="com.skywatcher.PanoramaApp.StepByStepActivity"></activity>
		<activity android:name="com.skywatcher.PanoramaApp.SettingActivity"></activity>
		<activity android:name="com.skywatcher.PanoramaApp.ScenarioSettingActivity"></activity>
		<activity android:name="com.skywatcher.PanoramaApp.ShootActivity"></activity>
		<activity android:name="com.skywatcher.PanoramaApp.SettingGuideActivity"></activity>
		<activity android:name="com.skywatcher.PanoramaApp.DeviceListActivity"></activity>
	</application>

	<uses-permission android:name="android.permission.BLUETOOTH"></uses-permission>
	<uses-permission android:name="android.permission.BLUETOOTH_ADMIN"></uses-permission>
	<uses-permission android:name="android.permission.SET_DEBUG_APP"></uses-permission>
</manifest> 